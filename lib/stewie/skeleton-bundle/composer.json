{
    "name": "stewie/skeleton-bundle",
    "type": "symfony-bundle",
    "description": "A skeleton bundle for Symfony5 bundle development",
    "authors" : [{
        "name" : "Stefan Wiegmann",
        "email" : "mail@stefanwiegmann.de"
    }],
    "keywords" : [
        "skeleton bundle symfony symfony5"
    ],
    "license" : [
        "MIT"
    ],
    "require": {
        "php": "^7.2.5|^8.0.9",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-pack": "*",
        "symfony/yaml": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "Stewie\\SkeletonBundle\\": ""
        }
    },
    "extra" : {
    "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}
