<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{% block titleBlock %}{% block titlePrefix %}symdev{% endblock titlePrefix %} {% block titleSuffix %}local{% endblock titleSuffix %}{% endblock titleBlock %}</title>
        {% block stylesheets %}
          <!-- <link href='https://fonts.googleapis.com/css?family=PT+Sans' rel='stylesheet' type='text/css'> -->
          <link href="{{ asset('bootstrap-4.4.1-dist/css/bootstrap.min.css') }}" rel="stylesheet">
          <link href="{{ asset('css/dev.css') }}" rel="stylesheet">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}" />
    </head>

    <body>

        {% block body %}body{% endblock body %}

      {% block javascripts %}
        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="{{ asset('jquery-3.4.1/jquery-3.4.1.min.js') }}"></script>
        <script src="{{ asset('popper-1.16.0/popper.min.js') }}"></script>
        <script src="{{ asset('bootstrap-4.4.1-dist/js/bootstrap.bundle.min.js') }}"></script>
        <script src="{{ asset('fontawesome-free-5.13.0-web/js/all.min.js') }}"></script>
        <script>
        $(function () {
          $('[data-toggle="tooltip"]').tooltip()
          })
        </script>
      {% endblock javascripts %}
    </body>

</html>
